{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "next/core-web-vitals",
        "plugin:react/recommended",
        "standard",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "react/prop-types": "warn",
      "react/react-in-jsx-scope": "off",
      "spaced-comment": "off",
      "no-unused-vars": "warn",
      "camelcase": "warn",
      "no-sequences": "off"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
}